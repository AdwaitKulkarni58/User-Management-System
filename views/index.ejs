<%- include("header") %>
<main id="main-site">
  <div class="container">
    <div class="box-nav d-flex justify-between">
      <a href="/new-user" class="border-shadow">
        <span class="text-gradient">
          New User <i class="fa-solid fa-user"></i
        ></span>
      </a>
    </div>
    <form action="/" method="POST">
      <table class="table">
        <thead class="thead-dark">
          <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <%- include("table.ejs") %>
        </tbody>
      </table>
    </form>
  </div>
</main>
<%- include("footer") %>
